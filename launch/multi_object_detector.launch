<?xml version="1.0"?>
<launch>

    <arg name="robot_id" default=""/>
    <param name="robot_id" value="$(arg robot_id)" />
    <node pkg="object_detector" name="multi_object_detector" type="multi_object_detector.py" output="screen" /> 

    <arg name="rviz" default="false" />
    <group if="$(arg rviz)">
        <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find object_detector)/rviz_cfg/object_detector.rviz" />
    </group>

</launch>
